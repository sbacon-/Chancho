<section class="feature">
<canvas id="canvas"></canvas>
<script src=index.js></script>
<button id="play" onclick="engine.startGame({onProgress: printProgress })">Click to Play</button>
<script>

var engine = new Engine({"args":[],"canvasResizePolicy":2,"executable":"index","experimentalVK":false,"fileSizes":{"index.pck":1885312,"index.wasm":17503191},"focusCanvas":true,"gdnativeLibs":[]});
var playButton = document.querySelector("#play");     
function printProgress(current, total) {
var percentage = (current/total)*100;
playButton.setAttribute("style","background:linear-gradient(180deg,var(--secondary-dsat) 0%,var(--secondary)"+percentage+"%, var(--complementary) "+percentage+"%,var(--complementary) 100%)"); 
console.log("Loaded " + current + " of " + total + " bytes");
}

</script>
